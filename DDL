DROP TABLE IF EXISTS Proteins;
DROP TABLE IF EXISTS Genes;

-- Create the Genes table
CREATE TABLE Genes (
    gene_id VARCHAR(30) PRIMARY KEY,
    gene_name VARCHAR(1000)
);

-- Create the Proteins table
CREATE TABLE Proteins (
    protein_id INT(11) AUTO_INCREMENT PRIMARY KEY,
    protein_name TEXT NOT NULL,
    gene_id VARCHAR(30) NOT NULL,
    protein_sequence TEXT NOT NULL,
    FOREIGN KEY (gene_id) REFERENCES Genes(gene_id)
);
